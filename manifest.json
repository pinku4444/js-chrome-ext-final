{
  "name": "JS Function Extension",
  "description": "Extension for getting JS Pre-Defined Functions",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "html/extension.html",
    "default_icon": "/images/ext.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens html/extension.html"
    }
  }
} 
